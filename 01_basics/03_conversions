console.log(typeof booleanIsLoggedIn)
*/
// conversion to Boolean
// 1 => true ; 0 => false
// "" => false
// "abc" => true

let someNumber = 33

console.log(someNumber)
console.log(typeof someNumber)
let stringNumber = String(someNumber)// converting to String
